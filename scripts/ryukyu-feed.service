
[Unit]
Description=ryukyu-feed
Wants=network.target
After=network.target

[Service]
User=ryukyu
RuntimeDirectory=ryukyu-feed
ExecStart=/usr/local/share/ryukyu.dix.asia/ryukyu-feed.sh
Type=simple
Restart=always
RestartSec=30
StartLimitInterval=1
StartLimitBurst=100
SyslogIdentifier=ryukyu-feed
Nice=-1

[Install]
WantedBy=default.target
